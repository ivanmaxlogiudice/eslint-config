{
    "name": "@ivanmaxlogiudice/eslint-config",
    "type": "module",
    "version": "4.0.3",
    "packageManager": "bun@1.3.5",
    "description": "Personal ESLint config",
    "license": "MIT",
    "homepage": "https://github.com/ivanmaxlogiudice/eslint-config#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanmaxlogiudice/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/ivanmaxlogiudice/eslint-config/issues"
    },
    "keywords": [
        "eslint-config"
    ],
    "exports": {
        ".": "./dist/index.mjs",
        "./cli": "./dist/cli.mjs",
        "./package.json": "./package.json"
    },
    "types": "./dist/index.d.mts",
    "bin": "./bin/index.mjs",
    "files": [
        "bin",
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "stub": "tsdown",
        "dev": "config-inspector --config eslint.config.ts",
        "build": "bun run typegen && tsdown --clean --dts",
        "test": "bun test",
        "lint": "eslint --fix",
        "typegen": "bun scripts/typegen.ts",
        "prepack": "bun run build",
        "release": "bumpp && npm publish",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "@unocss/eslint-plugin": "^66.6.0",
        "eslint": "^10.0.0",
        "eslint-plugin-regexp": "^3.0.0",
        "eslint-plugin-yml": "^3.1.2",
        "yaml-eslint-parser": "^2.0.0"
    },
    "peerDependenciesMeta": {
        "@unocss/eslint-plugin": {
            "optional": true
        },
        "eslint-plugin-regexp": {
            "optional": true
        },
        "eslint-plugin-yml": {
            "optional": true
        },
        "yaml-eslint-parser": {
            "optional": true
        }
    },
    "dependencies": {
        "@antfu/install-pkg": "^1.1.0",
        "@clack/prompts": "^1.0.1",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
        "@eslint/markdown": "^7.5.1",
        "@ivanmaxlogiudice/gitignore": "^0.0.2",
        "@stylistic/eslint-plugin": "^5.8.0",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "@vitest/eslint-plugin": "^1.6.9",
        "ansis": "^4.2.0",
        "cac": "^6.7.14",
        "eslint-plugin-antfu": "^3.2.2",
        "eslint-plugin-import-lite": "^0.5.1",
        "eslint-plugin-jsdoc": "^62.5.5",
        "eslint-plugin-jsonc": "^2.21.1",
        "eslint-plugin-n": "^17.24.0",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^5.5.0",
        "eslint-plugin-unicorn": "^63.0.0",
        "eslint-plugin-unused-imports": "^4.4.1",
        "eslint-plugin-vue": "^10.8.0",
        "globals": "^17.3.0",
        "jsonc-eslint-parser": "^2.4.2",
        "local-pkg": "^1.1.2",
        "tsdown": "^0.20.3",
        "vue-eslint-parser": "^10.4.0"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^1.4.2",
        "@types/bun": "^1.3.9",
        "@types/node": "^25.2.3",
        "@unocss/eslint-plugin": "^66.6.0",
        "bumpp": "^10.4.1",
        "eslint": "^10.0.0",
        "eslint-plugin-regexp": "^3.0.0",
        "eslint-plugin-yml": "^3.1.2",
        "eslint-typegen": "^2.3.0",
        "nano-staged": "^0.9.0",
        "simple-git-hooks": "^2.13.1",
        "typescript": "^5.9.3",
        "unbuild": "^3.6.1",
        "yaml-eslint-parser": "^2.0.0"
    },
    "resolutions": {
        "eslint": "^10.0.0"
    },
    "simple-git-hooks": {
        "pre-commit": "./node_modules/.bin/nano-staged"
    },
    "nano-staged": {
        "*": "eslint --fix"
    }
}
