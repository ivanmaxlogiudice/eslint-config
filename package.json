{
    "name": "@ivanmaxlogiudice/eslint-config",
    "version": "1.0.0-beta.3",
    "packageManager": "pnpm@8.6.10",
    "description": "Personal ESLint config",
    "type": "module",
    "license": "MIT",
    "homepage": "https://github.com/ivanmaxlogiudice/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/ivanmaxlogiudice/eslint-config/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanmaxlogiudice/eslint-config.git"
    },
    "files": [
        "index.cjs",
        "index.js",
        "src"
    ],
    "main": "./index.js",
    "module": "./index.js",
    "exports": {
        "require": "./index.cjs",
        "import": "./index.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "lint": "eslint . --fix",
        "release": "bumpp && pnpm publish",
        "prepublishOnly": "pnpm run build"
    },
    "peerDependencies": {
        "eslint": "^8.0.0"
    },
    "dependencies": {
        "@eslint/js": "^8.45.0",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-define-config": "^1.21.0",
        "eslint-plugin-antfu": "^0.39.8",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsonc": "^2.9.0",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-no-only-tests": "^3.1.0",
        "eslint-plugin-unicorn": "^48.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "eslint-plugin-vue": "^9.15.1",
        "eslint-plugin-yml": "^1.8.0",
        "globals": "^13.20.0",
        "jsonc-eslint-parser": "^2.3.0",
        "local-pkg": "^0.4.3",
        "vue-eslint-parser": "^9.3.1",
        "yaml-eslint-parser": "^1.2.2"
    },
    "devDependencies": {
        "@types/node": "^20.4.4",
        "bumpp": "^9.1.1",
        "eslint": "^8.45.0",
        "tsup": "^7.1.0",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=16.14.0"
    }
}
