{
    "name": "@ivanmaxlogiudice/eslint-config",
    "type": "module",
    "version": "3.1.2",
    "packageManager": "bun@1.2.4",
    "description": "Personal ESLint config",
    "license": "MIT",
    "homepage": "https://github.com/ivanmaxlogiudice/eslint-config#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanmaxlogiudice/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/ivanmaxlogiudice/eslint-config/issues"
    },
    "keywords": ["eslint-config"],
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "./bin/index.js",
    "files": ["bin", "dist"],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "stub": "tsup",
        "dev": "config-inspector --config eslint.config.ts",
        "build": "bun run typegen && tsup --clean --dts",
        "test": "vitest",
        "lint": "eslint --fix",
        "typegen": "bun scripts/typegen.ts",
        "prepack": "bun run build",
        "release": "bumpp && npm publish",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "@unocss/eslint-plugin": "^0.62.3",
        "eslint": "^9.10.0",
        "eslint-plugin-regexp": "^2.6.0",
        "eslint-plugin-yml": "^1.14.0",
        "yaml-eslint-parser": "^1.2.3"
    },
    "peerDependenciesMeta": {
        "@unocss/eslint-plugin": {
            "optional": true
        },
        "eslint-plugin-markdown": {
            "optional": true
        },
        "eslint-plugin-regexp": {
            "optional": true
        },
        "eslint-plugin-yml": {
            "optional": true
        },
        "yaml-eslint-parser": {
            "optional": true
        }
    },
    "dependencies": {
        "@antfu/install-pkg": "^1.0.0",
        "@clack/prompts": "^0.10.0",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
        "@eslint/markdown": "^6.3.0",
        "@ivanmaxlogiudice/gitignore": "^0.0.2",
        "@stylistic/eslint-plugin": "^4.2.0",
        "@stylistic/eslint-plugin-migrate": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "@vitest/eslint-plugin": "^1.1.38",
        "eslint-plugin-antfu": "^3.1.1",
        "eslint-plugin-import-x": "^4.9.3",
        "eslint-plugin-jsdoc": "^50.6.9",
        "eslint-plugin-jsonc": "^2.20.0",
        "eslint-plugin-n": "^17.17.0",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^4.10.1",
        "eslint-plugin-unicorn": "^58.0.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "eslint-plugin-vue": "^10.0.0",
        "globals": "^16.0.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^1.1.1",
        "picocolors": "^1.1.1",
        "vue-eslint-parser": "^10.1.1",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^1.0.2",
        "@types/node": "^22.13.14",
        "@types/yargs": "^17.0.33",
        "@unocss/eslint-plugin": "^66.0.0",
        "bumpp": "^10.1.0",
        "bun-types": "^1.2.7",
        "eslint": "^9.23.0",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-regexp": "^2.7.0",
        "eslint-plugin-yml": "^1.17.0",
        "eslint-typegen": "^2.1.0",
        "lint-staged": "^15.5.0",
        "simple-git-hooks": "^2.12.1",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "unbuild": "^3.5.0",
        "vitest": "^3.0.9",
        "yaml-eslint-parser": "^1.3.0"
    },
    "resolutions": {
        "@types/mdast": "^4.0.4",
        "eslint": "^9.23.0"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    }
}
