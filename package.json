{
    "name": "@ivanmaxlogiudice/eslint-config",
    "version": "1.0.0-beta.6",
    "packageManager": "pnpm@8.7.0",
    "description": "Personal ESLint config",
    "type": "module",
    "license": "MIT",
    "homepage": "https://github.com/ivanmaxlogiudice/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/ivanmaxlogiudice/eslint-config/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanmaxlogiudice/eslint-config.git"
    },
    "files": [
        "index.cjs",
        "index.js",
        "src"
    ],
    "main": "./index.js",
    "module": "./index.js",
    "exports": {
        "import": "./index.js",
        "require": "./index.cjs"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "lint": "eslint . --fix",
        "release": "bumpp && pnpm publish",
        "prepublishOnly": "pnpm run build"
    },
    "peerDependencies": {
        "eslint": "^8.0.0"
    },
    "dependencies": {
        "@eslint/js": "^8.48.0",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "@unocss/eslint-plugin": "^0.55.3",
        "eslint-config-standard": "^17.1.0",
        "eslint-define-config": "^1.23.0",
        "eslint-plugin-antfu": "^0.41.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsonc": "^2.9.0",
        "eslint-plugin-markdown": "^3.0.1",
        "eslint-plugin-n": "^16.0.2",
        "eslint-plugin-no-only-tests": "^3.1.0",
        "eslint-plugin-unicorn": "^48.0.1",
        "eslint-plugin-unused-imports": "^3.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "eslint-plugin-yml": "^1.8.0",
        "globals": "^13.21.0",
        "jsonc-eslint-parser": "^2.3.0",
        "local-pkg": "^0.4.3",
        "vue-eslint-parser": "^9.3.1",
        "yaml-eslint-parser": "^1.2.2"
    },
    "devDependencies": {
        "@types/node": "^20.5.7",
        "bumpp": "^9.2.0",
        "eslint": "^8.48.0",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.14.0"
    }
}
