{
    "name": "@ivanmaxlogiudice/eslint-config",
    "type": "module",
    "version": "3.1.10",
    "packageManager": "bun@1.2.21",
    "description": "Personal ESLint config",
    "license": "MIT",
    "homepage": "https://github.com/ivanmaxlogiudice/eslint-config#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ivanmaxlogiudice/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/ivanmaxlogiudice/eslint-config/issues"
    },
    "keywords": ["eslint-config"],
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "./bin/index.js",
    "files": ["bin", "dist"],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "stub": "tsdown",
        "dev": "config-inspector --config eslint.config.ts",
        "build": "bun run typegen && tsdown --clean --dts",
        "test": "bun test",
        "lint": "eslint --fix",
        "typegen": "bun scripts/typegen.ts",
        "prepack": "bun run build",
        "release": "bumpp && npm publish",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "@unocss/eslint-plugin": "^66.5.4",
        "eslint": "^9.38.0",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-yml": "^1.19.0",
        "yaml-eslint-parser": "^1.3.0"
    },
    "peerDependenciesMeta": {
        "@unocss/eslint-plugin": {
            "optional": true
        },
        "eslint-plugin-regexp": {
            "optional": true
        },
        "eslint-plugin-yml": {
            "optional": true
        },
        "yaml-eslint-parser": {
            "optional": true
        }
    },
    "dependencies": {
        "@antfu/install-pkg": "^1.1.0",
        "@clack/prompts": "^0.11.0",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
        "@eslint/markdown": "^7.4.1",
        "@ivanmaxlogiudice/gitignore": "^0.0.2",
        "@stylistic/eslint-plugin": "^5.5.0",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "@vitest/eslint-plugin": "^1.3.23",
        "ansis": "^4.2.0",
        "cac": "^6.7.14",
        "eslint-plugin-antfu": "^3.1.1",
        "eslint-plugin-import-lite": "^0.3.0",
        "eslint-plugin-jsdoc": "^61.1.5",
        "eslint-plugin-jsonc": "^2.21.0",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^4.15.1",
        "eslint-plugin-unicorn": "^61.0.2",
        "eslint-plugin-unused-imports": "^4.3.0",
        "eslint-plugin-vue": "^10.5.1",
        "globals": "^16.4.0",
        "jsonc-eslint-parser": "^2.4.1",
        "local-pkg": "^1.1.2",
        "tsdown": "^0.15.9",
        "vue-eslint-parser": "^10.2.0"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^1.3.0",
        "@types/bun": "^1.3.0",
        "@types/node": "^24.9.1",
        "@unocss/eslint-plugin": "^66.5.4",
        "bumpp": "^10.3.1",
        "eslint": "^9.38.0",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-yml": "^1.19.0",
        "eslint-typegen": "^2.3.0",
        "nano-staged": "^0.8.0",
        "simple-git-hooks": "^2.13.1",
        "typescript": "^5.9.3",
        "unbuild": "^3.6.1",
        "yaml-eslint-parser": "^1.3.0"
    },
    "resolutions": {
        "eslint": "^9.38.0"
    },
    "simple-git-hooks": {
        "pre-commit": "./node_modules/.bin/nano-staged"
    },
    "nano-staged": {
        "*": "eslint --fix"
    }
}
